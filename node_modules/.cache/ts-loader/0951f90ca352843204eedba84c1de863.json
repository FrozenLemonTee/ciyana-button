ing.loop < 3) {\n      playSetting.loop += 1\n    } else {\n      playSetting.loop = 0\n    }\n  }\n\n  const infoDate = inject('infoDate') as Ref<Mark | null>\n  const changeShowInfo = () => {\n    playSetting.showInfo = !playSetting.showInfo\n    document.body.scrollTop = 0\n    document.documentElement.scrollTop = 0\n    infoDate.value = null\n  }\n\n  return {\n    isError,\n    title,\n    playSetting,\n    infoDate,\n    Control: {\n      randomPlay,\n      stopPlay,\n      overlapChange,\n      autoRandomChange,\n      loopChange,\n      changeShowInfo\n    }\n  }\n}\n\nexport default {\n  components: {\n    Loading,\n    Error\n  },\n  setup() {\n    const { t } = useI18n()\n    const {\n      isError,\n      title,\n      playSetting,\n      infoDate,\n      Control\n    } = initControl()\n\n    return {\n      ACTION_I18N,\n      t,\n      isError,\n      title,\n      playSetting,\n      infoDate,\n      Control\n    }\n  }\n}\n"],"sourceRoot":""}]}
Cb,E;qBACE;AAAA,aA8BM,CA9BN,YAAA,CA8BM,KA9BN,EAAA,UAAA,EA8BM,CA7BJ,YAAA,CAAU,iBAAV,CA6BI,EA5BJ,YAAA,CAAU,iBAAV,CA4BI,EA3BJ,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,YAAA,CAAS,gBAAT,CACI,CAFN,E;;AAAA,OA2BI,EAxBJ,YAAA,CAIO,eAJP,EAIO,IAJP,EAIO;yBAHL;AAAA,iBAEM,CAFN,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,gBAAA,CADD,MAAA,CAAA,KACC,CAFN,EACU;AAAA;AADV,WAEM,CAFN;AAAA,S,CAGK;;;;AAAA,OAJP,CAwBI,EAnBJ,YAAA,CAkBO,eAlBP,EAkBO,IAlBP,EAkBO;yBAjBL;AAAA,iBAgBM,CAhBN,YAAA,CAgBM,KAhBN,EAAA,UAAA,EAgBM,CAfJ,YAAA,CAKE,cALF,EAKE;AAJA,YAAA,KAAK,EAAC,KAIN;AAHC,YAAA,IAAI,EAAE,MAAA,CAAA,CAAA,CAAE,MAAA,CAAA,SAAA,CAAU,MAAZ,CAGP;AAFA,YAAA,GAAG,EAAC,4DAEJ;AADA,YAAA,UAAU,EAAC;AACX,WALF,E,IAAA,E;;AAAA,Y,QAAA,CAeI,G,kBATJ,YAAA,CAQW,SARX,EAQW,IARX,EAQW,WAAA,CARc,MAAA,CAAA,gBAQd,EAR8B,UAAxB,IAAwB,EAApB;iCACnB,YAAA,CAME,cANF,EAME;mBAP6C,IAAI,CAAC,IAOlD;AALA,cAAA,KAAK,EAAC,KAKN;AAJC,cAAA,IAAI,EAAC,IAAI,CAAC,SAAL,CAAe,MAAA,CAAA,MAAf,CAIN;AAHC,cAAA,GAAG,EAAE,IAAI,CAAC,GAGX;AAFC,cAAA,KAAK,EAAE,IAAI,CAAC,KAEb;AADC,cAAA,UAAU,EAAE,IAAI,CAAC;AAClB,aANF,E,IAAA,E;;AAAA,c,sCAAA,C;WAOS,CARX,E;;AAAA,WASI,EAhBN,CAgBM,CAhBN;AAAA,S,CAiBK;;;;AAAA,OAlBP,CAmBI,CA9BN,CA8BM,CA9BN;AAAA,K;;;;GADF,C","sourcesContent":["<template>\r\n  <transition name=\"fade\" appear>\r\n    <div class=\"index\">\r\n      <Search />\r\n      <Readme />\r\n      <div ref=\"voice\">\r\n        <Voice />\r\n      </div>\r\n      <Card>\r\n        <div style=\"text-align: center\">\r\n          {{ total }}\r\n        </div>\r\n      </Card>\r\n      <Card>\r\n        <div class=\"content\">\r\n          <Btn\r\n            class=\"btn\"\r\n            :name=\"t(INFO_I18N.vtbbtn)\"\r\n            url=\"https://vbup-osc.github.io/vtuber-voice-button-collection/\"\r\n            background=\"rgb(255，255，255)\"\r\n          />\r\n          <template v-for=\"item in friendlyLinkList\" :key=\"item.name\">\r\n            <Btn\r\n              class=\"btn\"\r\n              :name=item.translate[locale]\r\n              :url=\"item.url\"\r\n              :color=\"item.color\"\r\n              :background=\"item.background\"\r\n            />\r\n          </template>\r\n        </div>\r\n      </Card>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport { inject, ref, watch, Ref, computed } from 'vue'\r\nimport { useI18n } from 'vue-i18n'\r\nimport { FriendlyLink, INFO_I18N, PlaySetting } from '@/assets/script/type'\r\nimport Search from '@/components/Search/SearchCard.vue'\r\nimport Readme from '@/components/Readme.vue'\r\nimport Voice from '@/components/Player/Player.vue'\r\nimport Card from '@/components/common/Card.vue'\r\nimport Btn from '@/components/common/Btn.vue'\r\nimport Setting from '@/../setting/setting.json'\r\n\r\n// 友链列表\r\nconst LINK: FriendlyLink[] = Setting['link'] || []\r\n\r\n/**\r\n * 切换分类模式时触发一次渐入动画\r\n */\r\nconst watchSettingChange = (playSetting: PlaySetting, voice: Ref<HTMLElement>) => {\r\n  let isRestart = false\r\n  watch(() => {\r\n    return playSetting.showInfo\r\n  }, () => {\r\n    if (!voice.value) return\r\n    if (isRestart) {\r\n      voice.value.style.animation = 'voice 0.5s'\r\n      isRestart = !isRestart\r\n    } else {\r\n      voice.value.style.animation = 'voice-restart 0.5s'\r\n      isRestart = !isRestart\r\n    }\r\n  })\r\n}\r\n\r\nexport default {\r\n  components: {\r\n    Search,\r\n    Readme,\r\n    Voice,\r\n    Card,\r\n    Btn\r\n  },\r\n  setup() {\r\n    const { t, locale } = useI18n()\r\n    const playSetting = inject('playSetting') as PlaySetting\r\n\r\n    const voice = ref() as Ref<HTMLElement>\r\n    watchSettingChange(playSetting, voice)\r\n\r\n    const total = computed(() => {\r\n      const msg = t(INFO_I18N.voiceTotalTip) + ': '\r\n      if (playSetting.showHide) {\r\n        return `${msg}${t(INFO_I18N.hideVoiceTotal)} ${t(INFO_I18N.hideLastDate) ? `(+${t(INFO_I18N.hideNewVoice)} · ${t(INFO_I18N.hideLastDate)})` : ''}`\r\n      } else {\r\n        return `${msg}${t(INFO_I18N.voiceTotal)} ${t(INFO_I18N.lastDate) ? `(+${t(INFO_I18N.newVoice)} · ${t(INFO_I18N.lastDate)})` : ''}`\r\n      }\r\n    })\r\n\r\n    return {\r\n      t,\r\n      locale,\r\n      friendlyLinkList: LINK,\r\n      voice,\r\n      total,\r\n      INFO_I18N\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"stylus\" scoped>\r\na\r\n  margin 0 5px\r\n\r\n.index\r\n  margin 0 auto\r\n  width 90%\r\n\r\n  .content\r\n    display flex\r\n    flex-wrap wrap\r\n\r\n@media only screen and (max-width 600px)\r\n  .index\r\n    width 95%\r\n</style>\r\n"],"sourceRoot":""}]}
CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;EACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;;EAEb,CAAC,CAAC,CAAC,CAAC;IACF,CAAC,CAAC,CAAC,EAAE,EAAE,EAAE,CAAC,CAAC,CAAC;IACZ,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;IACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;IACjB,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE;;IAEpB,CAAC,CAAC,CAAC;MACD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC;MACd,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;MACV,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC","file":"D:/FrozenLemonTee/Arbeit/WebProject/ciyana-button@2.0/ciyana-button/src/components/Readme.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <Collapse title=\"README\">\r\n    <div class=\"text\" v-if=\"README.length > 0\" v-html=\"README\"></div>\r\n    <div class=\"readme-icon\">\r\n      <div class=\"line\" v-if=\"!playSetting.nowPlay\">\r\n        <ControlIcon type=\"randomPlay\" />\r\n        <div class=\"tip\">{{ t(README_I18N.randomPlay) }}</div>\r\n      </div>\r\n      <div class=\"line\" v-else>\r\n        <ControlIcon type=\"stopPlay\" />\r\n        <div class=\"tip\">{{ t(README_I18N.stop) }}</div>\r\n      </div>\r\n      <div class=\"line\">\r\n        <ControlIcon type=\"overlapChange\" />\r\n        <div class=\"tip\">{{ overlap }}</div>\r\n      </div>\r\n      <div class=\"line\">\r\n        <ControlIcon type=\"autoRandomChange\" />\r\n        <div class=\"tip\">{{ autoRandom }}</div>\r\n      </div>\r\n      <div class=\"line\">\r\n        <ControlIcon type=\"loopChange\" />\r\n        <div class=\"tip\">{{ loop }}</div>\r\n      </div>\r\n      <div class=\"line\">\r\n        <ControlIcon type=\"changeShowInfo\" />\r\n        <div class=\"tip\">{{ showInfo }}</div>\r\n      </div>\r\n    </div>\r\n  </Collapse>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Collapse from './common/Collapse.vue'\r\nimport ControlIcon from './Control/ControlIcon.vue'\r\nimport { inject, computed } from 'vue'\r\nimport { useI18n } from 'vue-i18n'\r\nimport { PlaySetting, README_I18N } from '@/assets/script/type'\r\nimport Setting from '@/../setting/setting.json'\r\n\r\nconst README = Setting['readme'] || ''\r\n\r\nexport default {\r\n  components: {\r\n    Collapse,\r\n    ControlIcon\r\n  },\r\n  setup() {\r\n    const { t } = useI18n()\r\n    const playSetting = inject('playSetting') as PlaySetting\r\n\r\n    const overlap = computed(() => {\r\n      return playSetting.overlap ? t(README_I18N.overlapOn) : t(README_I18N.overlapOff)\r\n    })\r\n\r\n    const autoRandom = computed(() => {\r\n      return playSetting.autoRandom ? t(README_I18N.autoRandomOn) : t(README_I18N.autoRandomOff)\r\n    })\r\n\r\n    const loop = computed(() => {\r\n      if (playSetting.loop === 1) {\r\n        return t(README_I18N.loop1)\r\n      } else if (playSetting.loop === 2) {\r\n        return t(README_I18N.loop2)\r\n      } else if (playSetting.loop === 3) {\r\n        return t(README_I18N.loop3)\r\n      } else {\r\n        return t(README_I18N.loop0)\r\n      }\r\n    })\r\n\r\n    const showInfo = computed(() => {\r\n      return playSetting.showInfo ? t(README_I18N.showInfoOn) : t(README_I18N.showInfoOff)\r\n    })\r\n\r\n    return {\r\n      README,\r\n      t,\r\n      README_I18N,\r\n      playSetting,\r\n      overlap,\r\n      autoRandom,\r\n      loop,\r\n      showInfo\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"stylus\" scoped>\r\n.text\r\n  margin 0 5px 10px 5px\r\n  padding-bottom 10px\r\n  border-bottom 1px solid #eeeeee\r\n\r\n.readme-icon\r\n  display flex\r\n  flex-wrap wrap\r\n\r\n  .line\r\n    flex 0 0 auto\r\n    display flex\r\n    align-items center\r\n    margin 5px 10px 5px 0\r\n\r\n    .tip\r\n      margin-left 5px\r\n      height 30px\r\n      line-height 30px\r\n</style>\r\n"]}]}